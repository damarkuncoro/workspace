<!DOCTYPE html>
<html class="h-full" data-kt-theme="true" data-kt-theme-mode="light" dir="ltr" lang="en">
<%= render 'partials/raw/head' %>

 <body class="antialiased flex h-full text-base text-foreground bg-background demo1 kt-sidebar-fixed kt-header-fixed">

  <%= render 'partials/raw/theme_mode' %>
  
  <!-- Page -->
  <!-- Main -->
  <div class="flex grow">
   
   <%= render 'partials/raw/sidebar' %>
   <!-- Wrapper -->
   <div class="kt-wrapper flex grow flex-col">
    <!-- Header -->
    <%= render 'partials/raw/body/header' %>
    <!-- End of Header -->
    <!-- Content -->
    <%#= render 'partials/raw/contents/wrapper_content' %> 
    <main class="grow pt-5" id="content" role="content">
    <!-- Container -->
     <div class="kt-container-fixed" id="contentContainer">
     <style>
      .hero-bg {
		background-image: url('/assets/media/images/2600x1200/bg-1.png');
	}
	.dark .hero-bg {
		background-image: url('/assets/media/images/2600x1200/bg-1-dark.png');
	}
     </style>
    <%= yield %>
    </div>
     <!-- End of Container -->
    
    </main>
    <!-- End of Content -->

    <!-- End of Content -->
    <!-- Footer -->
    <%= render 'partials/raw/footer' %> 
   </div>
   <!-- End of Wrapper -->
  </div>
  <!-- End of Main -->

<%= render 'partials/raw/modals' %>
  <!-- End of Page -->
  
  <!-- Scripts -->
  <%= render 'partials/raw/scripts' %>
  <!-- End of Scripts -->
 </body>
</html>
